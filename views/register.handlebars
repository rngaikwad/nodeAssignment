<!DOCTYPE html>
<html>
<head>
{{>head}}
</head>
<body>
 <div class="navbar navbar-default navbar-fixed-top">
    <div class="container">
      <ul class="nav navbar-nav">
      	 <li><a href="/adminlandingpage">Home</a></li>
        <li><a href="/registerForm">Register</a></li>
        <li><a href="/userdetailsForm">User Details</a></li>
         <li><a href="/feedbackdetailsForm"> Feedback Details</a></li>
        <li><a href="/adminlogout">Logout</a></li>
      </ul>
    </div>
</div>
 <div class="container" style="margin-top:70px!important;">
	<div class="row">
	  <div class="col-md-4 col-md-offset-4 panel panel-default">
		<h1 align="center" class="margin-base-vertical">Sign up </h1>
		<h5 align="center" class="margin-base-vertical"><code>{{errorMessage}}</code></h5>

		<form class="margin-base-vertical" method="post" action="/register">

		 <p class="input-group ">
			<span class="input-group-addon"><span class="icon-user"></span></span>
			<input type="text" required class="form-control input-lg" name="name" placeholder=" name" />
		  </p>

		  <p class="input-group ">
			<span class="input-group-addon"><span class="icon-user"></span></span>
			<input type="text" required class="form-control input-lg" name="location" placeholder="location" />
		  </p>

		  <p class="input-group ">
			<span class="input-group-addon"><span class="icon-user"></span></span>
			<input type="text" required class="form-control input-lg" name="username" placeholder="login id" />
		  </p>

      <p class="input-group ">
			<span class="input-group-addon"><span class="icon-key"></span></span>
			<input type="password" required class="form-control input-lg" name="password" placeholder="password" />
		  </p>

		  <p class="text-center">
			<button type="submit" class="btn btn-info btn-lg">Create Account</button>
		  </p>
		</form>

		<!-- <div class="margin-base-vertical">
		  <h5 align="center">Already registered? <a href="/"> LogIn</a></h5>
		</div> -->

		<br/>
	 </div>
	</div>
   </div>

	{{>jsIncludes}}
</body>
</html>
