<html>
<head>
{{>head}}
</head>
<body>

 <div class="container">
  <div class="row">
  
  <h5 align="center" class="margin-base-vertical"><code>{{errorMessage}}</code></h5>
  <br><br><br>
  <div class="col-md-4">
  </div>

  <div class="col-md-4" style="border: solid 1px!important;background-color:grey!important;margin-top:40px!important">
	
	<form class="margin-base-vertical" method="post" action="/auth">
          <p class="input-group " style="margin-top:40px!important;">
			<span class="input-group-addon"><span class="icon-envelope"></span></span>
			<input type="text" required class="form-control input-lg" name="nm" placeholder="Username" />
		  </p>
          <p class="input-group ">
			<span class="input-group-addon"><span class="icon-key"></span></span>
			<input type="password" required class="form-control input-lg" name="pwd" placeholder="Password" />
		  </p>
          <br/>
		  <p class="text-center">
			<button type="submit" class="btn btn-warning btn-lg">Log In</button>
		  </p>
	</form>
	<br><br>
	</div>
	<div class="col-md-4">
  </div>
   </div>
 </div>

{{>jsIncludes}}
</body>
</html>